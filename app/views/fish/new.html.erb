<h1>Add Fish To Tank</h1>

<%= form_for [@tank, @fish] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label :breed %>
  <%= f.collection_select :breed_id, Breed.all, :id, :name %>
  <%= link_to "Create Breed", new_breed_path %><br><br>
  <%= f.label :purchase_store %>
  <%= f.text_field :purchase_store %><br><br>
  <%= f.label :purchase_date %>
  <%= f.text_field :purchase_date %><br><br>
  <%= f.label :cost %>
  <%= f.text_field :cost %><br><br>
  <%= f.submit "Add Fish to Tank" %>
<% end %>
